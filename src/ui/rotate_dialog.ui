<?xml version='1.0' encoding='utf-8'?>
<interface version="1.4" author="PygubuDesigner 0.40.1">
  <project>
    <settings>
      <setting id="name">Rotate Dialog</setting>
      <setting id="description">Used for initiating a rotation animation in the Wizard window.</setting>
      <setting id="module_name">rotate_dialog</setting>
      <setting id="template">application</setting>
      <setting id="main_widget">frame_rotate</setting>
      <setting id="main_classname">RotateDialog</setting>
      <setting id="main_menu" />
      <setting id="output_dir" />
      <setting id="output_dir2" />
      <setting id="import_tkvariables">False</setting>
      <setting id="use_ttk_styledefinition_file">False</setting>
      <setting id="use_i18n">False</setting>
      <setting id="all_ids_attributes">False</setting>
      <setting id="generate_code_onsave">False</setting>
      <setting id="use_window_centering_code">False</setting>
      <setting id="ttk_style_definition_file" />
    </settings>
    <customwidgets>
      <customwidget path="../custom_pygubu_widgets.py" />
    </customwidgets>
  </project>
  <object class="ttk.Frame" id="frame_rotate" named="True">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="frame_inner" named="True">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="ttk.Frame" id="frame_alias" named="True">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">2</property>
              <property name="sticky">w</property>
            </layout>
            <child>
              <object class="ttk.Label" id="lbl_alias" named="True">
                <property name="text" translatable="yes">&lt;type&gt; alias:</property>
                <property name="textvariable">string:v_alias_title</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="lvnauthwidgets.entrylimitwidget" id="entry_alias" named="True">
                <property name="textvariable">string:v_alias</property>
                <property name="width">30</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="frame_rotate_direction" named="True">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">15</property>
              <property name="row">3</property>
              <property name="sticky">w</property>
            </layout>
            <child>
              <object class="ttk.Label" id="lbl_rotate_direction" named="True">
                <property name="text" translatable="yes">Rotate direction:</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Radiobutton" id="rb_clockwise" named="True">
                <property name="text" translatable="yes">Clockwise</property>
                <property name="value">clockwise</property>
                <property name="variable">string:v_direction</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">1</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Radiobutton" id="cb_counterclockwise" named="True">
                <property name="text" translatable="yes">Counterclockwise</property>
                <property name="value">counterclockwise</property>
                <property name="variable">string:v_direction</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="frame_rotation_speed" named="True">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">4</property>
              <property name="sticky">ew</property>
            </layout>
            <containerlayout manager="grid">
              <property type="col" id="0" name="weight">1</property>
              <property type="row" id="1" name="weight">1</property>
            </containerlayout>
            <child>
              <object class="ttk.Label" id="lbl_rotation_speed" named="True">
                <property name="text" translatable="yes">Rotation speed &lt;scale info&gt;:</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.LabeledScale" id="scale_speed" named="True">
                <property name="variable">int:v_speed</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">1</property>
                  <property name="sticky">ew</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="frame_rotate_until" named="True">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">15</property>
              <property name="row">5</property>
              <property name="sticky">w</property>
            </layout>
            <containerlayout manager="grid">
              <property type="col" id="0" name="weight">1</property>
              <property type="row" id="1" name="weight">1</property>
            </containerlayout>
            <child>
              <object class="ttk.Label" id="lbl_stop_rotating_title" named="True">
                <property name="text" translatable="yes">Stop rotating when the angle reaches... (0 to 359)</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="columnspan">2</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.LabeledScale" id="scale_rotate_until" named="True">
                <property name="from_">0</property>
                <property name="to">359</property>
                <property name="variable">int:v_rotate_until</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">1</property>
                  <property name="sticky">ew</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Checkbutton" id="cb_rotate_forever" named="True">
                <property name="text" translatable="yes">Rotate forever</property>
                <property name="variable">boolean:v_rotate_forever</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">2</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
