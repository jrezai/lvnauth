<?xml version='1.0' encoding='utf-8'?>
<interface version="1.4" author="PygubuDesigner 0.40.1">
  <project>
    <settings>
      <setting id="name">Tint Dialog</setting>
      <setting id="description">Used for tint related commands in the Wizard window.</setting>
      <setting id="module_name">tint_dialog</setting>
      <setting id="template">application</setting>
      <setting id="main_widget">frame_tint</setting>
      <setting id="main_classname">TintDialog</setting>
      <setting id="main_menu" />
      <setting id="output_dir" />
      <setting id="output_dir2" />
      <setting id="import_tkvariables">False</setting>
      <setting id="use_ttk_styledefinition_file">False</setting>
      <setting id="use_i18n">False</setting>
      <setting id="all_ids_attributes">False</setting>
      <setting id="generate_code_onsave">False</setting>
      <setting id="use_window_centering_code">False</setting>
      <setting id="ttk_style_definition_file" />
    </settings>
    <customwidgets>
      <customwidget path="../custom_pygubu_widgets.py" />
    </customwidgets>
  </project>
  <object class="ttk.Frame" id="frame_tint" named="True">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="frame_inner" named="True">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="0" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="ttk.Frame" id="frame_alias" named="True">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
            </layout>
            <containerlayout manager="grid">
              <property type="col" id="0" name="weight">1</property>
            </containerlayout>
            <child>
              <object class="ttk.Label" id="lbl_alias" named="True">
                <property name="text" translatable="yes">&lt;type&gt; alias:</property>
                <property name="textvariable">string:v_alias_title</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="lvnauthwidgets.entrylimitwidget" id="entry_alias" named="True">
                <property name="textvariable">string:v_alias</property>
                <property name="width">30</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="frame_speed" named="True">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">15</property>
              <property name="row">2</property>
              <property name="sticky">w</property>
            </layout>
            <containerlayout manager="grid">
              <property type="col" id="1" name="weight">1</property>
              <property type="row" id="1" name="weight">1</property>
            </containerlayout>
            <child>
              <object class="ttk.Label" id="lbl_speed" named="True">
                <property name="text" translatable="yes">Gradual tinting animation speed:</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="columnspan">2</property>
                  <property name="row">0</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Spinbox" id="sb_speed" named="True">
                <property name="from_">1</property>
                <property name="textvariable">int:v_speed</property>
                <property name="to">100</property>
                <property name="width">5</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">1</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="label3">
                <property name="text" translatable="yes">(1 to 100)</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="padx">5</property>
                  <property name="row">1</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="frame5">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">0 15</property>
              <property name="row">3</property>
              <property name="sticky">w</property>
            </layout>
            <containerlayout manager="grid">
              <property type="col" id="1" name="weight">1</property>
              <property type="row" id="1" name="weight">1</property>
            </containerlayout>
            <child>
              <object class="ttk.Label" id="lbl_tint_amount" named="True">
                <property name="text" translatable="yes">Tint amount to stop at:</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="columnspan">2</property>
                  <property name="row">0</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Spinbox" id="sb_tint_amount" named="True">
                <property name="from_">1</property>
                <property name="textvariable">int:v_tint_amount</property>
                <property name="to">255</property>
                <property name="width">5</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">1</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="label5">
                <property name="text" translatable="yes">(0 to 255)</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="padx">5</property>
                  <property name="row">1</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="lbl_tint_amount_info" named="True">
                <property name="text" translatable="yes">lbl_tint_amount_info</property>
                <property name="textvariable">string:v_tint_amount_info</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="columnspan">2</property>
                  <property name="pady">5 0</property>
                  <property name="row">2</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Labelframe" id="labelframe1">
            <property name="height">200</property>
            <property name="text" translatable="yes">Tint type</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">5</property>
              <property name="row">4</property>
              <property name="sticky">ew</property>
            </layout>
            <child>
              <object class="ttk.Radiobutton" id="rb_darken" named="True">
                <property name="text" translatable="yes">Dark tint</property>
                <property name="value">dark</property>
                <property name="variable">string:v_tint_type</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">5</property>
                  <property name="pady">5</property>
                  <property name="row">1</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Radiobutton" id="rb_glow" named="True">
                <property name="text" translatable="yes">Bright tint</property>
                <property name="value">bright</property>
                <property name="variable">string:v_tint_type</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">5</property>
                  <property name="pady">0 5</property>
                  <property name="row">2</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
