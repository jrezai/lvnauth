<?xml version='1.0' encoding='utf-8'?>
<interface version="1.4" author="PygubuDesigner 0.40.1">
  <project>
    <settings>
      <setting id="name">Dialog for the remote call command</setting>
      <setting id="description">This window is used for the command: &lt;remote_call&gt;</setting>
      <setting id="module_name">remote_call</setting>
      <setting id="template">widget</setting>
      <setting id="main_widget">frame_call</setting>
      <setting id="main_classname">RemoteCallWindow</setting>
      <setting id="main_menu" />
      <setting id="output_dir" />
      <setting id="output_dir2" />
      <setting id="import_tkvariables">False</setting>
      <setting id="use_ttk_styledefinition_file">False</setting>
      <setting id="use_i18n">False</setting>
      <setting id="all_ids_attributes">False</setting>
      <setting id="generate_code_onsave">False</setting>
      <setting id="use_window_centering_code">False</setting>
      <setting id="ttk_style_definition_file" />
    </settings>
    <customwidgets>
      <customwidget path="../custom_pygubu_widgets.py" />
    </customwidgets>
  </project>
  <object class="ttk.Frame" id="frame_call" named="True">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="row">2</property>
      <property name="sticky">w</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="frame_action_name" named="True">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
          <property name="sticky">ew</property>
        </layout>
        <child>
          <object class="ttk.Label" id="lbl_custom_action" named="True">
            <property name="text" translatable="yes">Custom action name:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="lvnauthwidgets.entrylimitwidget" id="entry_custom_action_name" named="True">
            <property name="textvariable">string:v_custom_action_name</property>
            <property name="width">25</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame_arguments" named="True">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="pady">10 0</property>
          <property name="row">1</property>
          <property name="sticky">ew</property>
        </layout>
        <child>
          <object class="ttk.Label" id="lbl_arguments" named="True">
            <property name="text" translatable="yes">(optional) - Arguments (separated by commas):
Example:
decision=yes, favfruit=orange</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="lvnauthwidgets.entrylimitwidget" id="entry_custom_action_arguments" named="True">
            <property name="textvariable">string:v_custom_arguments</property>
            <property name="width">40</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
